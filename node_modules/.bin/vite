#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chenrongqi/毕设/项目/vue3-front-components/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chenrongqi/毕设/项目/vue3-front-components/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/registry.npmmirror.com+vite@3.2.6_sass@1.62.0+terser@5.17.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/registry.npmmirror.com+vite@3.2.6_sass@1.62.0+terser@5.17.1/node_modules/vite/bin/vite.js" "$@"
fi
